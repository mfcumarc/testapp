{
  "font": {
    "weight": {
      "default": {
        "$type": "fontWeight",
        "$value": 400
      }
    },
    "style": {
      "body": {
        "$value": "Open Sans",
        "$type": "fontFamily"
      },
      "display": {
        "$value": "Bungee (400)",
        "$type": "fontFamily"
      }
    }
  },
  "color": {
    "primary": {
      "$type": "color",
      "$value": "#E03ED7"
    },
    "secondary": {
      "$type": "color",
      "$value": "#801B7A"
    },
    "accent": {
      "$type": "color",
      "$value": "#89D3FF"
    },
    "pure-white": {
      "$type": "color",
      "$value": "#FFFFFF"
    },
    "dark": {
      "$type": "color",
      "$value": "#000000"
    }
  },
  "button": {
    "primary":{
      "background": {
        "$value": "{color.primary}",
      },
      "title": {
        "$value": "{font.default}"
      }
    }
  }
}
